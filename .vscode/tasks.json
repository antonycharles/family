{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "buildteamaccountsmanagementweb",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/team.accounts.management.web/src/Team.Accounts.Management.Web/Team.Accounts.Management.Web.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "buildteamaccountsapi",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/team.accounts.api/src/Team.Accounts.Api/Team.Accounts.Api.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "--project",
                "${workspaceFolder}/Teste.Authentication.Api.csproj"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}