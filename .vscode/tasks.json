{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "buildaccountsmanagementweb",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/.accounts.management.web/src/Accounts.Management.Web/Accounts.Management.Web.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "buildaccountsapi",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/.accounts.api/src/Accounts.Api/Accounts.Api.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "--project",
                "${workspaceFolder}/Teste.Authentication.Api.csproj"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}