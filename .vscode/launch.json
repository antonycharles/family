    "version": "0.2.0",
    "configurations": [
        {
            "name": "Team.Accounts.Gestao.Web",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildteamaccountsmanagementweb",
            "program": "${workspaceFolder}/team.accounts.management.web/src/Team.Accounts.Management.Web/bin/Debug/net8.0/Team.Accounts.Management.Web.dll",
            "args": [],
            "cwd": "${workspaceFolder}/team.accounts.management.web",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/team.accounts.management.web/src/Team.Accounts.Management.Web/Views"
            }
        },
        {
            "name": "Team.Accounts.Api",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildteamaccountsapi",
            "program": "${workspaceFolder}/team.accounts.api/src/Team.Accounts.Api/bin/Debug/net8.0/Team.Accounts.Api.dll",
            "args": [],
            "cwd": "${workspaceFolder}/team.accounts.api/src/Team.Accounts.Api/",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/team.accounts.api/src/Team.Accounts.Api/Views"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ],
    "compounds": [
    {
      "name": "Accounts",
      "configurations": [
        "Team.Accounts.Gestao.Web",
        "Team.Accounts.Api"
      ],
      "stopAll": true
    }
  ]
}