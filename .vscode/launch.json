{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Accounts.Gestao.Web",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildaccountsmanagementweb",
            "program": "${workspaceFolder}/.accounts.management.web/src/Accounts.Management.Web/bin/Debug/net8.0/Accounts.Management.Web.dll",
            "args": [],
            "cwd": "${workspaceFolder}/.accounts.management.web",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/.accounts.management.web/src/Accounts.Management.Web/Views"
            }
        },
        {
            "name": "Accounts.Api",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildaccountsapi",
            "program": "${workspaceFolder}/.accounts.api/src/Accounts.Api/bin/Debug/net8.0/Accounts.Api.dll",
            "args": [],
            "cwd": "${workspaceFolder}/.accounts.api/src/Accounts.Api/",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/.accounts.api/src/Accounts.Api/Views"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ],
    "compounds": [
    {
      "name": "Accounts",
      "configurations": [
        "Accounts.Gestao.Web",
        "Accounts.Api"
      ],
      "stopAll": true
    }
  ]
}